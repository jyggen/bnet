{
    "name": "pwnraid/bnet",
    "description": "Library for Battle.net Web APIs",
    "keywords": ["Battle.net", "Blizzard", "Warcraft", "Starcraft", "Diablo"],
    "homepage": "https://github.com/pwnraid/bnet",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jyggen@pwnraid.org"
        },
        {
            "name": "pwnRaid Community",
            "homepage": "https://github.com/pwnraid/bnet/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0",
        "league/oauth2-client": "1.0-beta1",
        "tedivm/stash": "0.12.*",
        "sebastian/version": "~1.0"
    },
    "require-dev": {
        "humbug/humbug": "~1.0",
        "ircmaxell/tuli": "dev-master",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Pwnraid\\Bnet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pwnraid\\Bnet\\Test\\": "test"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
